<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginPageController">
   <children>
      <Button layoutX="117.0" layoutY="287.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="33.0" prefWidth="135.0" text="Sign In" />
      <Button layoutX="340.0" layoutY="287.0" mnemonicParsing="false" onAction="#SignUpPressed" prefHeight="33.0" prefWidth="135.0" text="Sign Up" />
      <PasswordField fx:id="password" layoutX="199.0" layoutY="217.0" prefHeight="25.0" prefWidth="171.0" promptText="Password" />
      <TextField fx:id="clubName" layoutX="199.0" layoutY="157.0" prefHeight="25.0" prefWidth="171.0" promptText="Club Name" />
      <ImageView fx:id="imageView" fitHeight="251.0" fitWidth="253.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/Controller/loginpage.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
